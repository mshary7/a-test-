                            ` : ''}

                        </section>
                    `).join('')}
                </main>

                <!-- Footer -->
                <footer style="background: linear-gradient(135deg, ${primaryColor} 0%, ${primaryColor}dd 100%); padding: 3rem 1.5rem; text-align: center; color: white;">
