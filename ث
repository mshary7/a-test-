                        `).join('')}
                    </div>
                </div>

                <!-- Main Content -->
                <main style="max-width: 1100px; margin: 0 auto; padding: 0 1.5rem 4rem 1.5rem;">
                    ${sections.map(section => `
                        <section id="${section.id}" class="card-hover content-fade-in" style="background: ${secondaryColor}; border-radius: 20px; padding: 2.5rem; margin-bottom: 2rem; box-shadow: 0 2px 12px rgba(0,0,0,0.06); border-right: 6px solid ${accentColor};">
                            
                            <div style="display: flex; align-items: center; gap: 1.25rem; margin-bottom: 2rem;">
                                <div class="section-number" style="background: ${primaryColor}15; color: ${primaryColor};">
                                    ${section.number}
                                </div>
                                <div style="flex: 1;">
                                    <h2 style="font-size: ${baseSize * 1.65}px; font-weight: 700; color: ${textColor}; margin: 0 0 0.3rem 0;">
                                        ${section.icon} ${section.title}
                                    </h2>
                                    <p style="font-size: ${baseSize * 0.9}px; color: #6b7280; margin: 0;">
